import pandas as pdimport matplotlib.pyplot as pltdf = pd.read_csv("https://raw.githubusercontent.com/UVADS/orientation-technical/main/dat/2022-06-14-survey.csv")df = df[df['RAM (in GB)'] < 256 ] #they dont make computers like they used to in 1970plt.hist(df['RAM (in GB)'], bins=16)plt.title("HistogRAM")plt.xlabel("RAM (in GB)")plt.ylabel("Frequency")#if anyone sees this follow me on instagram @david.siamon <3